<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
  <form-beans >
    <form-bean name="strutsOneForm" type="org.apache.struts.action.DynaActionForm" >
		<form-property name="userName" type="java.lang.String"></form-property>
		<form-property name="pwd" type="java.lang.String"></form-property>
	</form-bean>
  </form-beans>
  <global-exceptions />
  <global-forwards />
  <action-mappings >
    <action
      attribute="strutsOneForm"
      name="strutsOneForm"
      input="/index.jsp"
      parameter="action"
      path="/wxJsSdkDemoAction"
      scope="request"
      type="myf.caption5.action.WxJsSdkDemoAction"
      cancellable="true">
      <forward name="success" path="/caption5/wxJsSdkDemo.jsp" />
    </action>
  </action-mappings>
</struts-config>

